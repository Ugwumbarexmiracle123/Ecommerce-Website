    <section id="hero">
        <h4>Trade-in-offer</h4>
        <h3>Super value deals</h3>
        <h1>On all products</h1>
        <p>Save more with coupons & up to 70% off!</p>
        <button>Shop Now</button>
    </section>

    <section id="feature" class="section-p1">
        <div class="fe-box">
            <img src="https://img.freepik.com/premium-vector/fast-delivery-truck-with-motion-lines-online-delivery-express-delivery-quick-move-fast-shipping-truck-apps-websites-cargo-van-moving-fast-chronometer-fast-distribution-service-24-7_435184-654.jpg?w=2000" alt="">
            <h6>Free Shipping</h6>
        </div>

        <div class="fe-box">
            <img src="https://fragrant.mobiletransaction.org/wp-content/uploads/2022/03/online-ordering-illustration.jpg" alt="">
            <h6>Online Order</h6>
        </div>

        <div class="fe-box">
            <img src="https://img.freepik.com/free-vector/family-couple-saving-money_74855-5240.jpg?w=2000" alt="">
            <h6>Save Money</h6>
        </div>

        <div class="fe-box">
            <img src="https://www.insperity.com/wp-content/uploads/Promotion_policy_1200x630-1024x538.png" alt="">
            <h6>Promotion</h6>
        </div>

        <div class="fe-box">
            <img src="https://elements-cover-images-0.imgix.net/61e0e52f-2f4f-4787-b7ca-3ed8aeeec517?auto=compress&crop=edges&fit=crop&fm=jpeg&h=630&w=1200&s=144d78623d81de169cf9c41a9c517ed5" alt="">
            <h6>Happy Sale</h6>
        </div>

        <div class="fe-box">
            <img src="https://fluentsupport.com/wp-content/uploads/2022/02/Difference-between-Customer-Support-and-Customer-Service-01-scaled.jpg" alt="">
            <h6>Support</h6>
        </div>

    </section>

    <section id="product1" class="section-p1">
        <h2>Featured Products</h2>
        <p>Rex Collection New Modern Design</p>
        <span>New Arrival</span>
        
        <div class="container-fluid">
            <div class="row">
              
          <% for (let i = 0; i < products.length; i++) { %>
           <% const product =products[i]; %>
           <% if (i > 7) break; %>
           <div class="col-md-3">
            <div class="pro-container">
              <div class="pro">
                <% if (typeof isAdmin !== 'undefined') { %>
                <div class="admin-controls">
                  <i
                    class="update-btn fa-solid fa-pen"
                    data-bs-toggle="modal"
                    data-bs-target="#updateModal"
                    onclick="updateItemToUpdate('<%= product._id %>')"
                  ></i>
                  <i
                    class="delete-btn fa-sharp fa-solid fa-trash"
                    data-bs-toggle="modal"
                    data-bs-target="#deleteItemModal"
                    onclick="updateItemToDelete('<%= product._id %>')"
                  ></i>
                </div>
                <% } %>
                <img height="170vh" src="<%= product.image %>" alt="" class="product-img" />
                <div class="des">
                  <h5><%= product.name %></h5>
                  <span class="star <%=product.rating %>"></span> <span class="star <%=product.rating %>"></span> 
                  <h4>$<%= product.price %></h4>
                </div>
                <a href="#" class="cart"><i  class="fa fa-shopping-cart" onclick="addToCart('<%= product._id %>')" ></i></a>
              </div>
            </div>
            </div>
          <% } %>
            </div>
          </div>

    </section>

    <section id="newsletter" class="section-p1">
      <div class="newstext">
        <h4>Sign Up For Newsletter</h4>
        <p>Get E-mail updates about our latest shop and <span>special offers</span></p>
      </div>
      <div class="form">
        <input type="text" placeholder="Your email address">
        <button>Sign Up</button>
      </div>
    </section>
